<server class="org.jpos.q2.iso.QServer" logger="Q2" name="HostISOServer">

    <attr name="port" type="java.lang.Integer">12261</attr>
    <attr name="maxSessions" type="java.lang.Integer">4000</attr>
    <attr name="minSessions" type="java.lang.Integer">10</attr>

    <channel name="ClientChannel" class="org.jpos.iso.channel.ASCIIChannel"
             logger="Q2"
             packager="org.jpos.iso.packager.GenericPackager">
        <property name="packager-config" value="config/iso2003AsciiPackager.xml" />
<!--        <property name="timeout" value="36000000"></property>-->
<!--        <property name="connect-timeout" value="10000"></property>-->
    </channel>

    <request-listener class="com.misc.iso.ChannelListener" logger="Q2">
        <!--<property name="space" value="tspace:mySpace" />-->
        <property name="space" value="tspace:mySpace"/>
        <property name="queue" value="TransactionQueue" />
        <property name="timeout" value="15000"/>
        <property name="ctx.DESTINATION" value="client-mux" />
    </request-listener>
</server>
